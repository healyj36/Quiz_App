<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healyj36.quizapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewQuestion"
            android:label="View question"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".AllQuestions"
            android:label="All questions"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".InfiniteGame"
            android:label="Infinite game mode"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".InfiniteModeStart"
            android:label="Infinite game mode started"
            android:theme="@style/AppTheme" >
        </activity>
        <activity android:name=".Online" >
        </activity>
    </application>

</manifest>
